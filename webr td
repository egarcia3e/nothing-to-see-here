insert into sandbox.edit_sf
	select
	deal_key,
	deal.permalink,
	load.opportunity_id,
	txnmy_cat_v3_name category_v3,
	case 
		when record_type_name = 'GrouponLive' then 'Live'
		when txnmy_cat_v3_name = 'Tickets' then 'Live'
		when record_type_name = 'Groupon Getaways' and opp.travel_quality not in ('5. No Getaways; No G1','5. Getaways for simultaneous G1','4. No Getaways; Approved for G1') then 'Getaways'
		when record_type_name = 'Groupon Getaways' and opp.travel_quality in ('5. No Getaways; No G1','5. Getaways for simultaneous G1','4. No Getaways; Approved for G1') then 'Local'
		when record_type_name = 'Groupon Getaways' then 'Getaways'
		when record_type_name = 'Groupon Goods' then 'Goods'
		when record_type_name = 'G1' then 'Local'
		when record_type_name = 'gLite' then 'Local'
		else record_type_name end channel,
	case
		when repeat_deal = 'New deal' then 'New'
		when repeat_deal is null and writeup <> 'New deal section only' then 'New'
		when clone_flag = 1 then 'Clone'
		else 'Refeature' end refeature,
	case opp.straight_to_private_sale 
		when 'Yes' then 'STPS'
		else 'Featured' end stps,
	cast(deal_vette_time at time zone 'America Central' as date) deal_vette_date,
	lead_time_date sure_thing_date,
	cast(case 
		when editor_ts > image_ts and cast(editor_ts at time zone 'America Central' as date) <= deal.actual_start_date then editor_ts
		when image_ts >= editor_ts and cast(image_ts at time zone 'America Central' as date) <= deal.actual_start_date then image_ts
		else null end at time zone 'America Central' as date) edit_setup_date,
	deal.actual_start_date go_live_date,
	case
	-- clones
		when clone_flag = 1 then 'Cloned'
	-- goods
		when writeup = 'Web: traditional' and record_type_name = 'Groupon Goods' then 'Traditional'
		when writeup in ('CC touchup','CC - no changes','CC - touchup') and record_type_name = 'Groupon Goods' then 'CC touchup'
		when writeup = 'Light touch' and record_type_name = 'Groupon Goods' then 'Light Touch'
		when writeup = 'Goods to Editor' and record_type_name = 'Groupon Goods' then 'Goods to Editor'
	-- travel
		when record_type_name = 'Groupon Getaways' and refeature = 'New' and opp.travel_quality = '1. Excellent Getaways Deal'	and new_area_section = 1 then 'Silver'
		when record_type_name = 'Groupon Getaways' and refeature = 'New' and opp.travel_quality = '1. Excellent Getaways Deal'	and new_area_section <> 1 then 'Bronze'
		when record_type_name = 'Groupon Getaways' and refeature = 'New' and opp.travel_quality = '3. Acceptable Getaways Deal' and new_area_section = 1 then 'Getaways: traditional'
		when record_type_name = 'Groupon Getaways' and refeature = 'New' and opp.travel_quality = '3. Acceptable Getaways Deal' and new_area_section <> 1 then 'Recycled area'
		when record_type_name = 'Groupon Getaways' and refeature = 'Refeature' and opp.travel_quality not in ('5. No Getaways; No G1','4. No Getaways; Approved for G1','5. Getaways for simultaneous G1') and new_area_section <> 1 then 'Old copy'
		when record_type_name = 'Groupon Getaways' and refeature = 'Refeature' and opp.travel_quality not in ('5. No Getaways; No G1','4. No Getaways; Approved for G1','5. Getaways for simultaneous G1') and new_area_section = 1 then 'Old copy, new area'
	-- live
		when (record_type_name = 'GrouponLive' or category_v3 = 'Tickets') and writeup = 'Web: traditional' and video = 'GrouponLive Video' then 'New show, new profile with video'
		when (record_type_name = 'GrouponLive' or category_v3 = 'Tickets') and writeup = 'Web: traditional' then 'New show, new profile'
		when (record_type_name = 'GrouponLive' or category_v3 = 'Tickets') and writeup = 'Light touch' and video = 'GrouponLive Video' then 'New show, old profile with video'
		when (record_type_name = 'GrouponLive' or category_v3 = 'Tickets') and writeup = 'Light touch' then 'New show, old profile'
		when (record_type_name = 'GrouponLive' or category_v3 = 'Tickets') and writeup = 'New deal section only' then 'Old show, old profile'
		when (record_type_name = 'GrouponLive' or category_v3 = 'Tickets') and writeup in ('CC no changes','CC - no changes','CC touchup','No changes') then 'No changes'
	-- local
		when editor_ts = deal_vette_time and record_type_name in ('G1','gLite') then 'DQ no changes'
		when writeup = 'CC - no changes' then 'No changes'
		when writeup = 'CC no changes' then 'No changes'
		when writeup = 'CC touchup' then 'CC touchup'
		when writeup = 'No changes' then 'No changes'
		when writeup = 'Full rewrite' then 'Web: traditional'
		when writeup = 'Web: traditional' then 'Web: traditional'
		when writeup = 'Phone: traditional' then 'Web: traditional'
		when writeup = 'Light touch' then 'Light touch'
		when writeup = 'MQI: baseball card' then 'MQI: baseball card'
		when writeup = 'MQI: Q&A' then 'MQI: Q&A'
		when writeup = 'MQI: traditional' then 'MQI: traditional'
		when writeup = 'New deal section only' then 'New deal section only'
		when writeup = 'Primer' then 'Primer'
		when record_type_name = 'Groupon Goods' then 'QA no changes'
		else 'Other' end writeup,
	profile_section,
	image_path,
	case when deal_creator in ('AAA - Deal Cloned by Dev','AAA-Deal Cloned by Dev','AAA-Evergreen','AAA - Deal Page Cloned','AAA - National Deals Team') then 1 else 0 end clone_flag,
	case when editor_ts = deal_vette_time then 1 else 0 end dq_copy_flag,
	case when record_type_name = 'Groupon Goods' and writeup not in ('CC touchup','CC - no changes','CC - touchup','Light touch','Web: traditional') then 1 else 0 end qa_copy_flag,
	case when record_type_name = 'Groupon Goods' and image_path is null then 1 else 0 end qa_image_flag,
	expedite_this,
	white_glove_type,
	vetter.full_name dq_name,
	deal_vette_time,
	cc_name,
	cc.cc_ts - cast(extract(timezone_hour from cast(cc.cc_ts as timestamp(0) format 'dd/mm/yyyybhh:mi:ss' with time zone at time zone 'America Central')) as interval hour) cc_ts,
	writer.full_name writer_name,
	detail_review_ready_ts writer_ts,
	editor.full_name editor_name,
	editor_ready_date editor_ts,
	image.full_name image_name,
	image_ready_ts image_ts,
	site_editor_name qa_name,
	final_copy_edit qa_ts,
	cast((case 
			when index(image_received_date,'-') <> 0
			then null
			when char_length(image_received_date) = 10 
	        then image_received_date
			when char_length(image_received_date) = 9 and position('/' in image_received_date) = 2
	        then '0' || image_received_date
			when char_length(image_received_date) = 9 and position('/' in image_received_date) = 3
	        then substr(image_received_date,1,3) || '0' || substr(image_received_date,4,9)
			when char_length(image_received_date) = 8
	        then '0' || substr(image_received_date,1,2) || '0' || substr(image_received_date,3,8) else null end) as date format 'mm/dd/yyyy') image_received_date,
	n_images,
	video,
	merchant_interview,
	interview_requested,
	goods_deal_source


	from sandbox.edit_sf_load load
	left join user_groupondw.dim_opportunity opp
	on load.opportunity_id = opp.opportunity_id
	left join dw.v_dim_deal deal
	on opp.opportunity_key = deal.opportunity_key
	left join user_groupondw.dim_sf_record_type typ
	on opp.record_type_key = typ.record_type_key
	left join dw.v_dim_groupon_person vetter
	on opp.deal_vetter = vetter.person_key
	left join dw.v_dim_groupon_person writer
	on opp.deal_writer_key = writer.person_key
	left join dw.v_dim_groupon_person editor
	on opp.deal_editor_key = editor.person_key
	left join dw.v_dim_groupon_person image
	on opp.image_designer_key = image.person_key
	left join (select opportunity_id,
					cast((case 
					when char_length(cc_ts) = 16 and char_length(trim(substr(cc_ts,1,10))) = 10 and char_length(trim(substr(cc_ts,12,16))) = 5 
						then cc_ts || ':00'
					when char_length(cc_ts) = 15 and char_length(trim(substr(cc_ts,1,10))) = 9 and char_length(trim(substr(cc_ts,11,15))) = 5 and position('/' in cc_ts) = 3 and position('/' in trim(substr(cc_ts,4,7))) = 2 
						then substr(cc_ts,1,3) || '0' || substr(cc_ts,4,15) || ':00'
					when char_length(cc_ts) = 15 and char_length(trim(substr(cc_ts,1,10))) = 9 and char_length(trim(substr(cc_ts,11,15))) = 5 and position('/' in cc_ts) = 2 and position('/' in trim(substr(cc_ts,4,7))) = 2 
						then '0' || cc_ts || ':00'
					when char_length(cc_ts) = 15 and char_length(trim(substr(cc_ts,1,10))) = 10 and char_length(trim(substr(cc_ts,11,15))) = 4 
						then substr(cc_ts,1,11) || '0' || substr(cc_ts,12,15) || ':00' 
					when char_length(cc_ts) = 14 and char_length(trim(substr(cc_ts,1,9))) = 9 and char_length(trim(substr(cc_ts,10,14))) = 4 and position('/' in cc_ts) = 2
						then '0' || substr(cc_ts,1,10) || '0' || substr(cc_ts,11,14) || ':00'
					when char_length(cc_ts) = 14 and char_length(trim(substr(cc_ts,1,9))) = 9 and char_length(trim(substr(cc_ts,10,14))) = 4 and position('/' in substr(cc_ts,4,6)) = 2
						then substr(cc_ts,1,3) || '0' || substr(cc_ts,4,7) || '0' || substr(cc_ts,11,14) || ':00'
					when char_length(cc_ts) = 14 and char_length(trim(substr(cc_ts,1,9))) = 8 and char_length(trim(substr(cc_ts,10,14))) = 5
						then '0' || substr(cc_ts,1,2) || '0' || substr(cc_ts,3,7) || substr(cc_ts,10,14) || ':00'
					when char_length(cc_ts) = 13 
						then '0' || trim(substr(cc_ts,1,2)) || '0' || trim(substr(cc_ts,3,7)) || ' 0' || trim(substr(cc_ts,10,13)) || ':00' end) as timestamp(0) format 'mm/dd/yyyybhh:mi:ss') cc_ts
				from sandbox.edit_sf_load) cc
	on load.opportunity_id = cc.opportunity_id
	
	where deal_key is not null
		and opp.self_service_deal <> 1;










insert into sandbox.edit_zd
select
    permalink,
    requester,
    your_department requester_dept,
    assignee,
    second_assignee,
    image_or_copy,
    national_multimarket,
    number_of_permas,
    reopens,
    replies,
    case when tags like '%s1%' or tags like '%ft%' then 1 else 0 end one_touch,
    created_at - cast(extract(timezone_hour from cast(created_at as timestamp(0) format 'dd/mm/yyyybhh:mi:ss' with time zone at time zone 'America Central')) as interval hour) created_at,
    assigned_at - cast(extract(timezone_hour from cast(assigned_at as timestamp(0) format 'dd/mm/yyyybhh:mi:ss' with time zone at time zone 'America Central')) as interval hour) assigned_at,
    solved_at - cast(extract(timezone_hour from cast(solved_at as timestamp(0) format 'dd/mm/yyyybhh:mi:ss' with time zone at time zone 'America Central')) as interval hour) solved_at,    
    se_error,
    tagging_notes,
    manager,
    override_reason,
    country_city,
    us_city,
    tags,
    cast(a.ticket_id as int) ticket_id,
    root_one,
    case when manager_override = 'Root 1' and split_part(override_verdict,'::',1) = 'Override Confirmed' 
        then null else fault_one end fault_one,
    root_two,
    case when manager_override = 'Root 2' and split_part(override_verdict,'::',1) = 'Override Confirmed' 
        then null else fault_two end fault_two,
    root_three,
    case when manager_override = 'Root 3' and split_part(override_verdict,'::',1) = 'Override Confirmed' 
        then null else fault_three end fault_three,
    root_four,
    case when manager_override = 'Root 4' and split_part(override_verdict,'::',1) = 'Override Confirmed' 
        then null else fault_four end fault_four,
    root_five,
    case when manager_override = 'Root 5' and split_part(override_verdict,'::',1) = 'Override Confirmed' 
        then null else fault_five end fault_five,
    liveroot_one,
    case when manager_override = 'Live Root 1' and split_part(override_verdict,'::',1) = 'Override Confirmed' 
        then null else livefault_one end livefault_one,
    liveroot_two,
    case when manager_override = 'Live Root 2' and split_part(override_verdict,'::',1) = 'Override Confirmed' 
        then null else livefault_two end livefault_two,
    liveroot_three,
    case when manager_override = 'Live Root 3' and split_part(override_verdict,'::',1) = 'Override Confirmed' 
        then null else livefault_three end livefault_three
from (select
        ticket_id,
        requester,
        submitter,
        assignee,
        tags,
        status,
        cast(reopens as smallint) as reopens,
        cast(replies as smallint) as replies,
        second_assignee,
        translate(deal_url using unicode_to_locale with error) as deal_url,
        country_city,
        us_city,
        case
                when image_or_copy = '{{dc.image_or_copy_-_copy}}' 
                        then 'Copy' 
                when image_or_copy = '{{dc.image_or_copy_-_image}}' 
                        then 'Image' 
                else image_or_copy end as image_or_copy,
        case 
                when your_department = '{{dc.your_department_-_sales}}' 
                        then 'Sales' 
                when your_department = '{{dc.your_department_-_plannin' 
                        then 'Planning/Scheduling' 
                when your_department = '{{dc.your_department_-_am_pm}}' 
                        then 'Account/Partner Management'       
                when your_department = '{{dc.your_department_-_sales_m' 
                        then 'Sales Manager'    
                when your_department = '{{dc.your_department_-_editori' 
                        then 'Editorial'
                when your_department = '{{dc.your_department_-_other}}'
                        then 'Other'
                else your_department end as your_department,
        national_multimarket,
        case number_of_permas when '5-Feb' then '2 to 5' when '10-Jun' then '6 to 10' else number_of_permas end as number_of_permas,
        tagging_notes,
        se_error,
        manager,
        manager_override,
        override_reason,
        override_verdict,
        case root_one
            when '-' then null
            when '{{dc.root-admin-_banner_request_at}}' then 'Admin:: Banner request'
            when '{{dc.root-admin-_deal_permalink}}' then 'Admin:: Deal Permalink'
            when '{{dc.root-admin-_deal_setup-_multi-voucher}}' then 'Admin:: Deal setup:: Multi-voucher'
            when '{{dc.root-admin-_deal_setup-_wrong_cdas_at}}' then 'Admin:: Deal setup:: Wrong CDAs'
            when '{{dc.root-admin-_deal_setup-_wrong_channel_formatting}}' then 'Admin:: Deal setup:: Wrong channel formatting'
            when '{{dc.root-admin-_deal_setup-_wrong_channel_option_private_sale_mulligan_at}}' then 'Admin:: Deal setup:: Wrong channel/option, private sale, mulligan'
            when '{{dc.root-admin-_deal_setup-_wrong_deal_run_at}}' then 'Admin:: Deal setup:: Wrong deal run'
            when '{{dc.root-admin-_deal_setup-_wrong_validity_at}}' then 'Admin:: Deal setup:: Wrong validity'
            when '{{dc.root-admin-_info_overlay_cms_page-_missing}}' then 'Admin:: Info overlay / CMS page:: Missing'
            when '{{dc.root-admin-_info_overlay_cms_page-_subjective_edit}}' then 'Admin:: Info overlay / CMS page:: Subjective edit'
            when '{{dc.root-admin-_info_overlay_cms_page-_typo}}' then 'Admin:: Info overlay / CMS page:: Typo / Formatting'
            when '{{dc.root-admin-_merchant_center}}' then 'Admin:: Merchant center'
            when '{{dc.root-admin-_site_bug}}' then 'Admin:: Site bug'
            when '{{dc.root-admin-_traits}}' then 'Admin:: Traits'
            when '{{dc.root-buy_button-_reorder}}' then 'Buy Button:: Reorder'
            when '{{dc.root-buy_button-_sell_price-_sell_price_change}}' then 'Buy Button:: Sell price change'
            when '{{dc.root-buy_button-_unit_value-_unit_value_change}}' then 'Buy Button:: Unit value change'
            when '{{dc.root-buy_button-_voucher_title-_wrong_info}}' then 'Buy Button:: Voucher title:: Inaccurate description:: Inaccurate details'
            when '{{dc.root-buy_button-_voucher_title-_inaccurate_description-_incorrect_salesforce}}' then 'Buy Button:: Voucher title:: Inaccurate description:: Incorrect Salesforce'
            when '{{dc.root-buy_button-_voucher_title-_subjective_edit}}' then 'Buy Button:: Voucher title:: Subjective edit'
            when '{{dc.root-buy_button-_voucher_title-_typo}}' then 'Buy Button:: Voucher title:: Typo / Formatting'
            when '{{dc.root-buy_button-_sell_price-_wrong_sell_price_at}}' then 'Buy Button:: Wrong sell price'
            when '{{dc.root-buy_button-_sell_price-_wrong_sell_price_in_salesforce_record}}' then 'Buy Button:: Wrong sell price in salesforce record'
            when '{{dc.root-buy_button-_unit_value-_wrong_value_at}}' then 'Buy Button:: Wrong unit value'
            when '{{dc.root-buy_button-_unit_value-_wrong_unit_value_in_salesforce_record}}' then 'Buy Button:: Wrong unit value in salesforce record'
            when '{{dc.root-deal_structure-_multi-deal_option-_added_option_location_post_vet}}' then 'Deal Structure:: Added option/location post-vette'
            when '{{dc.root-deal_structure-_cap_change}}' then 'Deal Structure:: Cap change'
            when '{{dc.root-deal_structure-_product_or_service_change}}' then 'Deal Structure:: Product or service change'
            when '{{dc.root-deal_structure-_multi-deal_option-_removing_option_location_post_vet}}' then 'Deal Structure:: Removed option/location post-vette'
            when '{{dc.root-deal_structure-_wrong_cap_global_at}}' then 'Deal Structure:: Wrong cap/global'
            when '{{dc.root-deal_structure-_wrong_product_or_service}}' then 'Deal Structure:: Wrong product or service'
            when '{{dc.root-fine_print-_fine_print_addition-_buyer_max}}' then 'Fine Print:: Addition:: Buyer max'
            when '{{dc.root-fine_print-_fine_print_addition-_deal_exceptions}}' then 'Fine Print:: Addition:: Deal exceptions'
            when '{{dc.root-fine_print-_expiration_date}}' then 'Fine Print:: Expiration Date'
            when '{{dc.root-fine_print-_fine_print_omission-_buyer_max}}' then 'Fine Print:: Fine Print entry error:: Buyer max'
            when '{{dc.root-fine_print-_fine_print_omission-_deal_exceptions}}' then 'Fine Print:: Fine Print entry error:: Deal exceptions'
            when '{{dc.root-fine_print-_removal}}' then 'Fine Print:: Removal'
            when '{{dc.root-fine_print-_typo}}' then 'Fine Print:: Typo / Formatting'
            when '{{dc.root-image-_cms-_option_missed}}' then 'Image:: CMS:: Option missed'
            when '{{dc.root-image-_cms-_option_not_provided}}' then 'Image:: CMS:: Option not provided'
            when '{{dc.root-image-_cms-_page_not_created}}' then 'Image:: CMS:: Page not created'
            when '{{dc.root-image-_cms-_page_not_requested}}' then 'Image:: CMS:: Page not requested'
            when '{{dc.root-image-_cms-_wrong_product_image_provided}}' then 'Image:: CMS:: Wrong product image provided'
            when '{{dc.root-image-_cms-_wrong_product_image_pulled}}' then 'Image:: CMS:: Wrong product image pulled'
            when '{{dc.root-image-_communication-_merchandising_not_followed}}' then 'Image:: Communication:: Merchandising not followed'
            when '{{dc.root-image-_communication-_request_provided_late}}' then 'Image:: Communication:: Request provided late'
            when '{{dc.root-image-_communication-_merchandising_invalid_vague}}' then 'Image:: Communication:: Request violates standards'
            when '{{dc.root-image-_edit_request_resolution-_cms-_option_missed}}' then 'Image:: Edit Request Resolution:: CMS:: Option missed'
            when '{{dc.root-image-_edit_request_resolution-_cms-_wrong_product_images_used}}' then 'Image:: Edit Request Resolution:: CMS:: Wrong product images used'
            when '{{dc.root-image-_edit_request_resolution-_inaccurate_subpar_image_used}}' then 'Image:: Edit Request Resolution:: Inaccurate/subpar image used'
            when '{{dc.root-image-_edit_request_resolution-_unauthorized_image_used}}' then 'Image:: Edit Request Resolution:: Unauthorized image used'
            when '{{dc.root-image-_edit_request_resolution-_unpublished_missed_request}}' then 'Image:: Edit Request Resolution:: Unpublished/missed request'
            when '{{dc.root-image-_legal-_unauthorized_image_provided}}' then 'Image:: Legal:: Unauthorized image provided'
            when '{{dc.root-image-_legal-_unauthorized_image_pulled}}' then 'Image:: Legal:: Unauthorized image pulled'
            when '{{dc.root-image-_publishing-_image_not_uploaded}}' then 'Image:: Publishing:: Image not uploaded'
            when '{{dc.root-image-_publishing-_missed_publish_schedule}}' then 'Image:: Publishing:: Missed publish/schedule'
            when '{{dc.root-image-_publishing-_wrong_image_uploaded}}' then 'Image:: Publishing:: Wrong image uploaded'
            when '{{dc.root-image-_quality-_better_image_overlooked}}' then 'Image:: Quality:: Better image overlooked'
            when '{{dc.root-image-_quality-_misleading}}' then 'Image:: Quality:: Misleading'
            when '{{dc.root-root-image-_quality-_offensive}}' then 'Image:: Quality:: Offensive'
            when '{{dc.root-image-_quality-_offer_change}}' then 'Image:: Quality:: Offer change'
            when '{{dc.root-image-_quality-_outdated_content_provided}}' then 'Image:: Quality:: Outdated content provided'
            when '{{dc.root-image-_quality-_outdated_content_pulled}}' then 'Image:: Quality:: Outdated content pulled'
            when '{{dc.root-image-_quality-_specialty_not_showcased_provided}}' then 'Image:: Quality:: Specialty not showcased (provided)'
            when '{{dc.root-image-_quality-_specialty_not_showcased_pulled}}' then 'Image:: Quality:: Specialty not showcased (pulled)'
            when '{{dc.root-image-_quality-_stock_swap}}' then 'Image:: Quality:: Stock swap'
            when '{{dc.root-image-_quality-_sub-par}}' then 'Image:: Quality:: Sub-par'
            when '{{dc.root-image-_quality-_wrong_product_service}}' then 'Image:: Quality:: Wrong product/service'
            when '{{dc.root-image-_slideshow_order-_request_contradicts_standards}}' then 'Image:: Slideshow order:: Request contradicts standards'
            when '{{dc.root-image-_slideshow_order-_slideshow_contradicts_standards}}' then 'Image:: Slideshow order:: Slideshow contradicts standards'
            when '{{dc.root-redemption-_booking_landing_page_at}}' then 'Redemption:: Booking landing page'
            when '{{dc.root-redemption-_changing_redemption_method}}' then 'Redemption:: Changing redemption method'
            when '{{dc.root-redemption-_check_out}}' then 'Redemption:: Check out'
            when '{{dc.root-redemption-_codes-_not_uploaded}}' then 'Redemption:: Codes:: Not uploaded'
            when '{{dc.root-redemption-_codes-_wrong_code_type}}' then 'Redemption:: Codes:: Wrong code type'
            when '{{dc.root-redemption-_incorrect_or_missing_relevant_contact_information}}' then 'Redemption:: Incorrect or missing relevant contact information'
            when '{{dc.root-redemption-_merchant_contact}}' then 'Redemption:: Merchant contact'
            when '{{dc.root-redemption-_missing_steps_in_redemption_instructions_i_e_make_a_reservation}}' then 'Redemption:: Missing steps in Redemption Instructions'
            when '{{dc.root-redemption-_scheduler}}' then 'Redemption:: Scheduler'
            when '{{dc.root-redemption-_typo}}' then 'Redemption:: Typo / Formatting'
            when '{{dc.root-redemption-_wrong_merchant_voucher_details_at}}' then 'Redemption:: Wrong merchant voucher details'
            when '{{dc.root-scheduling-_deal_not_scheduled}}' then 'Scheduling:: Deal not scheduled'
            when '{{dc.root-scheduling-_deal_pull}}' then 'Scheduling:: Deal pull'
            when '{{dc.root-scheduling-_feature_date_change}}' then 'Scheduling:: Feature date change'
            when '{{dc.root-scheduling-_opt_in_deal_bank}}' then 'Scheduling:: Opt in deal bank'
            when '{{dc.root-scheduling-_opt_in_prefeature}}' then 'Scheduling:: Opt in prefeature'
            when '{{dc.root-scheduling-_opt_out_deal_bank}}' then 'Scheduling:: Opt out deal bank'
            when '{{dc.root-scheduling-_opt_out_prefeature}}' then 'Scheduling:: Opt out prefeature'
            when '{{dc.root-scheduling-_pause}}' then 'Scheduling:: Pause'
            when '{{dc.root-scheduling-_priority_position_at}}' then 'Scheduling:: Priority/position'
            when '{{dc.root-scheduling-_relevancy}}' then 'Scheduling:: Relevancy'
            when '{{dc.root-scheduling-_feature_date_change}}' then 'Scheduling:: Reschedule'
