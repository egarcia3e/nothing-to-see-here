            when liveroot_three = '{{dc.root-writeup-_wrong_copy}}' then 'Editor'
            when liveroot_three = '{{dc.root-zendesk-_duplicate_ticket}}' then 'Sales'
            when liveroot_three = '{{dc.root-zendesk-_entered_wrong_deal_sell_price_or_value}}' then 'Egregious Site Editor'
